 const logoutHandler = useCallback(() => {
    dispatch(authActions.logout());
    props.navigation.navigate('Login');
  }, [dispatch]);

  const onMyProfile = useCallback(() => {
    console.log('on My profile function is called');
    props.navigation.push('MyProfile');
  }, []);

  useEffect(() => {
    props.navigation.setParams({
      logoutHandlerPointer: logoutHandler,
    });
  }, [logoutHandler]);

  useEffect(() => {
    props.navigation.setParams({onMyProfilePointer: onMyProfile});
  }, []);